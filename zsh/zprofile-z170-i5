
# JModelica
JMODELICA="$HOME/Developer/jmodelica/bin"
set_path_var JMODELICA

# LaTeX
LATEX="/usr/local/texlive/2016/bin/x86_64-linux"
set_path_var LATEX

INFOPATH="${INFOPATH}:/usr/local/texlive/2016/texmf-dist/doc/info"
MANPATH="${MANPATH}:/usr/local/texlive/2016/texmf-dist/doc/man"

# node.js
NODE_JS="$HOME/bin/node/bin"
set_path_var NODE_JS

# minizing
MINIZINC="$HOME/bin/minizinc"
set_path_var MINIZINC

# Go
GOPATH="/usr/local/go/bin"
set_path_var GOPATH
export GOPATH="$HOME/Developer/go"

# autojump
. /usr/share/autojump/autojump.sh

fpath=(~/.zsh/completion $fpath)
autoload -Uz compinit && compinit -i

